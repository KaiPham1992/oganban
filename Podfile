# platform :ios, '9.0'
def main_pods
    pod 'SwiftyJSON'
    pod 'Alamofire', '4.7.3'
    pod 'ObjectMapper'
    pod 'SDWebImage'
    pod 'IQKeyboardManagerSwift'
    pod 'Firebase/Core'
    pod 'GoogleMaps'
    pod 'GooglePlaces'
    pod 'FBSDKLoginKit'
    pod 'FBSDKCoreKit'
    #pod 'FBSDKCoreKit', :git => 'https://github.com/facebook/facebook-objc-sdk.git', :branch => 'master'
    pod 'GoogleSignIn'
    pod 'DropDown', '2.3.12'
    pod 'CryptoSwift','0.15.0'
    pod 'Firebase/Messaging'
    pod 'ImageSlideshow'
    pod 'ImageSlideshow/SDWebImage'
    pod 'Google-Mobile-Ads-SDK'
    pod 'ImagePicker',:git => 'https://github.com/appsailor/ImagePicker.git', :branch => 'feature/Swift-4.2'
    pod 'Fabric'
    pod 'Crashlytics'
    pod 'ZaloSDK'
end

target 'Oganban-Dev' do
    use_frameworks!
    main_pods
end

target 'Oganban-AppStore' do
    use_frameworks!
    main_pods
end

target 'Oganban-Prod' do
    use_frameworks!
    main_pods
end
